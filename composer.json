{
    "name": "maks3w/phpunit-methods-trait",
    "description": "Provide PHPUnit method signatures as traits for IDE autocomplete",
    "keywords": [
        "autocomplete",
        "phpunit"
    ],
    "license": "BSD-2-Clause",
    "homepage": "https://github.com/Maks3w/PhpUnitMethodsTrait",
    "require": {
        "php": ">= 5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.7",
        "zendframework/zend-code": "^2.5.1"
    },
    "autoload": {
        "psr-4": {
            "Maks3w\\PhpUnitMethodsTrait\\": "dist/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maks3w\\PhpUnitMethodsTrait\\": [
                "dist/",
                "src/"
            ],
            "Maks3w\\PhpUnitMethodsTraitTest\\": [
                "test/"
            ]
        }
    }
}
